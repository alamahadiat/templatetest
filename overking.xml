<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:css='false' b:defaultwidgetversion='2' b:layoutsVersion='3' b:responsive='true' b:templateUrl='YOUR_GITHUB_CSS_URL' b:templateVersion='1.0' expr:dir='data:blog.languageDirection' expr:lang='data:blog.locale' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>

<head>
    <meta charset='UTF-8'/>
    <meta content='width=device-width, initial-scale=1.0' name='viewport'/>
    
    <!-- TEMPLATE TITLE & META -->
    <b:include data='blog' name='all-head-content'/>
    <title><data:view.title.escaped/></title>
    
    <!-- EXTERNAL CSS - GANTI DENGAN URL GITHUB ANDA -->
    <link href='YOUR_GITHUB_CSS_URL/style.css' rel='stylesheet' type='text/css'/>
    
    <!-- EXTERNAL JS - GANTI DENGAN URL GITHUB ANDA -->
    <script src='YOUR_GITHUB_JS_URL/script.js' type='text/javascript'></script>
    
    <!-- FONT AWESOME UNTUK ICONS -->
    <link href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css' rel='stylesheet'/>
</head>

<body>
    <!-- HEADER SECTION -->
    <header class='site-header'>
        <div class='container'>
            <div class='header-content'>
                <!-- LOGO/BRAND -->
                <div class='brand'>
                    <a href='/' class='brand-link'>
                        <h1 class='brand-title'>OverKing</h1>
                        <span class='brand-description'>Description blogger</span>
                    </a>
                </div>
                
                <!-- SEARCH BOX -->
                <div class='search-container'>
                    <form class='search-form' action='/search' method='get'>
                        <input class='search-input' name='q' placeholder='Search articles...' type='text'/>
                        <button class='search-btn' type='submit'>
                            <i class='fas fa-search'></i>
                        </button>
                    </form>
                </div>
                
                <!-- SOCIAL MEDIA -->
                <div class='social-media'>
                    <a class='social-link' href='#' target='_blank'>
                        <i class='fab fa-facebook-f'></i>
                    </a>
                    <a class='social-link' href='#' target='_blank'>
                        <i class='fab fa-twitter'></i>
                    </a>
                </div>
            </div>
        </div>
    </header>

    <!-- NAVIGATION -->
    <nav class='main-nav'>
        <div class='container'>
            <ul class='nav-menu'>
                <li class='nav-item'><a class='nav-link' href='/'>Home</a></li>
                <li class='nav-item'><a class='nav-link' href='/p/about-us.html'>About Us</a></li>
                <li class='nav-item'><a class='nav-link' href='/search/label/CRYPTO'>Crypto</a></li>
                <li class='nav-item'><a class='nav-link' href='/search/label/AIRDROP'>Airdrop</a></li>
                <li class='nav-item'><a class='nav-link' href='/search/label/BELAJAR'>Belajar</a></li>
                <li class='nav-item'><a class='nav-link' href='#'>Tools</a></li>
            </ul>
        </div>
    </nav>

    <!-- MAIN CONTENT -->
    <main class='main-content'>
        <div class='container'>
            <div class='content-wrapper'>
                
                <!-- LEFT CONTENT AREA -->
                <div class='content-area'>
                    
                    <!-- TRENDING SECTION (HOMEPAGE ONLY) -->
                    <b:if cond='data:view.isHomepage'>
                        <section class='trending-section'>
                            <div class='section-header'>
                                <h2 class='section-title'>TRENDING</h2>
                                <div class='nav-arrows'>
                                    <button class='nav-arrow prev'><i class='fas fa-chevron-left'></i></button>
                                    <button class='nav-arrow next'><i class='fas fa-chevron-right'></i></button>
                                </div>
                            </div>
                            <div class='trending-posts'>
                                <b:loop values='data:posts' var='post' index='postIndex'>
                                    <b:if cond='data:postIndex &lt; 5'>
                                        <article class='trending-post'>
                                            <div class='post-image'>
                                                <img expr:alt='data:post.title' expr:src='data:post.featuredImage.isResizable ? resizeImage(data:post.featuredImage, 300, &quot;1:1&quot;) : data:post.featuredImage'/>
                                            </div>
                                            <div class='post-content'>
                                                <div class='post-category'>
                                                    <b:loop values='data:post.labels' var='label' index='labelIndex'>
                                                        <b:if cond='data:labelIndex == 0'>
                                                            <span class='category-tag' expr:data-label='data:label.name'><data:label.name/></span>
                                                        </b:if>
                                                    </b:loop>
                                                </div>
                                                <h3 class='post-title'><a expr:href='data:post.url'><data:post.title/></a></h3>
                                                <div class='post-meta'>
                                                    <span class='post-date'><data:post.date/></span>
                                                    <span class='post-author'>By <data:post.author.name/></span>
                                                </div>
                                            </div>
                                        </article>
                                    </b:if>
                                </b:loop>
                            </div>
                        </section>
                    </b:if>

                    <!-- LATEST POSTS SECTION -->
                    <section class='latest-section'>
                        <div class='section-header'>
                            <h2 class='section-title'>
                                <b:if cond='data:view.isHomepage'>Latest</b:if>
                                <b:if cond='data:view.isPost'>Related Posts</b:if>
                                <b:if cond='data:view.isLabelSearch'>Posts in <data:view.search.label/></b:if>
                                <b:if cond='data:view.isSearch'>Search Results</b:if>
                            </h2>
                        </div>
                        
                        <!-- HOMEPAGE POSTS -->
                        <b:if cond='data:view.isHomepage'>
                            <div class='posts-grid'>
                                <b:loop values='data:posts' var='post' index='postIndex'>
                                    <b:if cond='data:postIndex &gt;= 5'>
                                        <article class='post-card'>
                                            <div class='post-image'>
                                                <img expr:alt='data:post.title' expr:src='data:post.featuredImage.isResizable ? resizeImage(data:post.featuredImage, 300, &quot;16:9&quot;) : data:post.featuredImage'/>
                                            </div>
                                            <div class='post-content'>
                                                <div class='post-category'>
                                                    <b:loop values='data:post.labels' var='label' index='labelIndex'>
                                                        <b:if cond='data:labelIndex == 0'>
                                                            <span class='category-tag' expr:data-label='data:label.name'><data:label.name/></span>
                                                        </b:if>
                                                    </b:loop>
                                                </div>
                                                <h3 class='post-title'><a expr:href='data:post.url'><data:post.title/></a></h3>
                                                <p class='post-excerpt'><data:post.snippet/></p>
                                                <div class='post-meta'>
                                                    <span class='post-date'><data:post.date/></span>
                                                    <span class='post-author'>By <data:post.author.name/></span>
                                                </div>
                                            </div>
                                        </article>
                                    </b:if>
                                </b:loop>
                            </div>
                        </b:if>
                        
                        <!-- SINGLE POST PAGE -->
                        <b:if cond='data:view.isPost'>
                            <b:loop values='data:posts' var='post'>
                                <article class='single-post'>
                                    <header class='post-header'>
                                        <div class='post-category'>
                                            <b:loop values='data:post.labels' var='label' index='labelIndex'>
                                                <b:if cond='data:labelIndex == 0'>
                                                    <span class='category-tag' expr:data-label='data:label.name'><data:label.name/></span>
                                                </b:if>
                                            </b:loop>
                                        </div>
                                        <h1 class='post-title'><data:post.title/></h1>
                                        <div class='post-meta'>
                                            <span class='post-date'><data:post.date/></span>
                                            <span class='post-author'>By <data:post.author.name/></span>
                                        </div>
                                    </header>
                                    
                                    <div class='post-image'>
                                        <img expr:alt='data:post.title' expr:src='data:post.featuredImage'/>
                                    </div>
                                    
                                    <div class='post-body'>
                                        <data:post.body/>
                                    </div>
                                    
                                    <!-- POST TAGS -->
                                    <div class='post-tags'>
                                        <b:loop values='data:post.labels' var='label'>
                                            <a class='tag-link' expr:href='data:label.url'><data:label.name/></a>
                                        </b:loop>
                                    </div>
                                    
                                    <!-- COMMENTS SECTION -->
                                    <b:if cond='data:post.allowComments'>
                                        <section class='comments-section'>
                                            <h3 class='comments-title'>Comments</h3>
                                            <b:include data='post' name='comments'/>
                                        </section>
                                    </b:if>
                                </article>
                            </b:loop>
                        </b:if>
                        
                        <!-- SEARCH/LABEL RESULTS -->
                        <b:if cond='data:view.isSearch or data:view.isLabelSearch'>
                            <div class='posts-grid'>
                                <b:loop values='data:posts' var='post'>
                                    <article class='post-card'>
                                        <div class='post-image'>
                                            <img expr:alt='data:post.title' expr:src='data:post.featuredImage.isResizable ? resizeImage(data:post.featuredImage, 300, &quot;16:9&quot;) : data:post.featuredImage'/>
                                        </div>
                                        <div class='post-content'>
                                            <div class='post-category'>
                                                <b:loop values='data:post.labels' var='label' index='labelIndex'>
                                                    <b:if cond='data:labelIndex == 0'>
                                                        <span class='category-tag' expr:data-label='data:label.name'><data:label.name/></span>
                                                    </b:if>
                                                </b:loop>
                                            </div>
                                            <h3 class='post-title'><a expr:href='data:post.url'><data:post.title/></a></h3>
                                            <p class='post-excerpt'><data:post.snippet/></p>
                                            <div class='post-meta'>
                                                <span class='post-date'><data:post.date/></span>
                                                <span class='post-author'>By <data:post.author.name/></span>
                                            </div>
                                        </div>
                                    </article>
                                </b:loop>
                            </div>
                        </b:if>
                    </section>
                    
                    <!-- LOAD MORE BUTTON -->
                    <b:if cond='data:view.isHomepage'>
                        <div class='load-more-container'>
                            <button class='load-more-btn' id='loadMoreBtn'>Load More</button>
                        </div>
                    </b:if>
                </div>

                <!-- SIDEBAR -->
                <aside class='sidebar'>
                    
                    <!-- FOLLOW US WIDGET -->
                    <div class='widget follow-widget'>
                        <h3 class='widget-title'>Follow Us</h3>
                        <div class='social-buttons'>
                            <a class='social-btn facebook' href='#' target='_blank'>
                                <i class='fab fa-facebook-f'></i>
                                <span>Facebook</span>
                            </a>
                            <a class='social-btn twitter' href='#' target='_blank'>
                                <i class='fab fa-twitter'></i>
                                <span>Twitter</span>
                            </a>
                        </div>
                    </div>
                    
                    <!-- CATEGORIES WIDGET -->
                    <div class='widget categories-widget'>
                        <h3 class='widget-title'>Kategory</h3>
                        <ul class='categories-list'>
                            <li><a href='/search/label/CRYPTO'>Crypto</a></li>
                            <li><a href='/search/label/AIRDROP'>Airdrop</a></li>
                            <li><a href='/search/label/BELAJAR'>Belajar</a></li>
                        </ul>
                    </div>
                    
                    <!-- POPULAR POSTS WIDGET -->
                    <div class='widget popular-widget'>
                        <h3 class='widget-title'>Populer Post</h3>
                        <div class='popular-posts'>
                            <b:loop values='data:posts' var='post' index='postIndex'>
                                <b:if cond='data:postIndex &lt; 5'>
                                    <article class='popular-post'>
                                        <div class='post-image'>
                                            <img expr:alt='data:post.title' expr:src='data:post.featuredImage.isResizable ? resizeImage(data:post.featuredImage, 80, &quot;1:1&quot;) : data:post.featuredImage'/>
                                        </div>
                                        <div class='post-content'>
                                            <h4 class='post-title'><a expr:href='data:post.url'><data:post.title/></a></h4>
                                            <span class='post-date'><data:post.date/></span>
                                        </div>
                                    </article>
                                </b:if>
                            </b:loop>
                        </div>
                    </div>
                    
                    <!-- YOU MIGHT LIKE WIDGET -->
                    <div class='widget might-like-widget'>
                        <h3 class='widget-title'>You Might Like</h3>
                        <div class='might-like-posts'>
                            <b:loop values='data:posts' var='post' index='postIndex'>
                                <b:if cond='data:postIndex &gt;= 5 and data:postIndex &lt; 10'>
                                    <article class='might-like-post'>
                                        <div class='post-image'>
                                            <img expr:alt='data:post.title' expr:src='data:post.featuredImage.isResizable ? resizeImage(data:post.featuredImage, 80, &quot;1:1&quot;) : data:post.featuredImage'/>
                                        </div>
                                        <div class='post-content'>
                                            <h4 class='post-title'><a expr:href='data:post.url'><data:post.title/></a></h4>
                                            <span class='post-date'><data:post.date/></span>
                                        </div>
                                    </article>
                                </b:if>
                            </b:loop>
                        </div>
                    </div>
                    
                    <!-- TOOLS WIDGET -->
                    <div class='widget tools-widget'>
                        <h3 class='widget-title'>Tools</h3>
                        <ul class='tools-list'>
                            <li><a href='#'>Tools 1</a></li>
                            <li><a href='#'>Tools 2</a></li>
                            <li><a href='#'>Tools 3</a></li>
                            <li><a href='#'>Tools 4</a></li>
                        </ul>
                    </div>
                </aside>
            </div>
        </div>
    </main>

    <!-- FOOTER -->
    <footer class='site-footer'>
        <div class='container'>
            <div class='footer-content'>
                <div class='footer-left'>
                    <p>&copy; 2025 OverKing. Create By 
                        <a href='#'>OverKing</a>
                    </p>
                </div>
                <div class='footer-right'>
                    <nav class='footer-nav'>
                        <a href='/p/disclaimer.html'>Disclaimer</a>
                        <a href='/p/contact-us.html'>Contact Us</a>
                        <a href='/p/privacy-policy.html'>Privacy Policy</a>
                        <a href='/p/about-us.html'>About Us</a>
                        <a href='/'>Home</a>
                    </nav>
                </div>
            </div>
        </div>
    </footer>

    <!-- BLOGGER REQUIRED SCRIPTS -->
    <b:include data='blog' name='google-analytics'/>
    
    <!-- PAGINATION -->
    <b:if cond='data:view.isHomepage'>
        <script>
            // PAGINATION SCRIPT AKAN DITAMBAHKAN DI SCRIPT.JS
        </script>
    </b:if>
</body>
</html>